@model IEnumerable<BL.Menu.MenuItem>

@foreach (var item in Model)
{
    <li class="nav-item me-1">
        <a class="nav-link text-white fw-medium small hover-nav py-0 px-2"
           asp-controller="@item.Controller"
           asp-action="@item.Action">
            @item.Title
        </a>
    </li>
}
